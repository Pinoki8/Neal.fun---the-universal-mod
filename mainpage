div><div class="tab-buttons"><button class="tab-btn active" data-tab="plant" disabled>Plant</button><button class="tab-btn" data-tab="head" disabled>Head Break</button><button class="tab-btn" data-tab="coffee" disabled>Coffee</button></div><div class="tab-content active" id="plant-tab"><div class="switch-container"><span>Water Plant</span><label class="switch"><input type="checkbox" id="plantSwitch" disabled><span class="slider"></span></label></div><div class="slider-container"><div class="slider-label"><span>Watering speed:</span><span id="speedLabel">1000</span> ms</div><input type="range" class="range-slider" id="speedSlider" min="100" max="1000" step="50" value="1000" disabled></div></div><div class="tab-content" id="head-tab"><div class="switch-container"><span>Break Neal's Head</span><label class="switch"><input type="checkbox" id="headSwitch" disabled><span class="slider"></span></label></div></div><div class="tab-content" id="coffee-tab"><div class="switch-container"><span>Buy him a coffee mode</span><label class="switch"><input type="checkbox" id="coffeeSwitch" disabled><span class="slider"></span></label></div></div><div class="customization"><div class="custom-title">Themes</div><div class="theme-buttons"><button class="theme-btn theme-1" disabled>Red-Black</button><button class="theme-btn theme-2" disabled>Purple-Blue</button></div></div>`;document.body.appendChild(y);let d=false,e=0,f=0,currentTheme=0;const intervals={plant:null,head:null};const originalMap=new WeakMap();function makeCoffeeButton(){const coffee=document.createElement('a');coffee.target="_blank";coffee.href="https://buymeacoffee.com/neal";coffee.className="social-btn social-full coffee-btn";coffee.setAttribute("data-v-cac35620","");coffee.innerHTML=`<img alt="Coffee" src="/general/coffee.svg" class="social-btn-icon" data-v-cac35620="">Buy me a coffee`;return coffee;}const selectors=['a.page-link','img.plant','svg.main-logo-img','svg.main-logo-img g','div.site-tagline','a.social-btn','div.social-btn','span.contact-label','a[href^="mailto:"]','a.privacy','div.ipr-container','div.made-by-desc','div.newsletter-tagline'];function buyCoffeeMode(){document.querySelectorAll(selectors.join(',')).forEach(el=>{if(!originalMap.has(el)){originalMap.set(el,el.cloneNode(true));}const coffeeBtn=makeCoffeeButton();originalMap.set(coffeeBtn,el.cloneNode(true));el.replaceWith(coffeeBtn);});}function restoreOriginal(){document.querySelectorAll('a.coffee-btn').forEach(btn=>{const original=originalMap.get(btn);if(original){btn.replaceWith(original);}});}function clickPlant(){const plant=document.querySelector('img.plant');if(plant)plant.click();}function startPlantClicking(){if(intervals.plant)clearInterval(intervals.plant);const slider=document.getElementById('speedSlider');intervals.plant=setInterval(clickPlant,slider.value);}let step=0,pos=0;const size=50;function moveMouseSquare(){const svg=document.querySelector('.my-head');if(!svg)return;const rect=svg.getBoundingClientRect();const centerX=rect.left+rect.width/2;const centerY=rect.top+rect.height/2;let x=centerX,y=centerY;switch(step){case 0:x+=pos;y-=size/2;break;case 1:x+=size/2;y+=pos-size/2;break;case 2:x-=pos;y+=size/2;break;case 3:x-=size/2;y-=pos-size/2;break;}const event=new MouseEvent('mousemove',{clientX:x,clientY:y,bubbles:true,cancelable:true,view:window});document.dispatchEvent(event);pos+=5;if(pos>size){pos=0;step=(step+1)%4;}}function updateThemeColors(){const themeColors=[{main:'#8B0000'},{main:'#6A0080'}];const current=themeColors[currentTheme];const style=document.createElement('style');style.id='dynamic-theme';const existingStyle=document.getElementById('dynamic-theme');if(existingStyle)existingStyle.remove();style.textContent=`input:checked+.slider{background-color:${current.main}!important}.range-slider::-webkit-slider-thumb{background:${current.main}!important}.range-slider::-moz-range-thumb{background:${current.main}!important}`;document.head.appendChild(style);}function updateMinimizeButtonTheme(){const minimizeBtn=y.querySelector('.minimize-btn');const themes=['linear-gradient(135deg,#8B0000,#000000)','linear-gradient(135deg,#6A0080,#1A0040)'];minimizeBtn.style.background=themes[currentTheme];}const paths=document.querySelectorAll('svg path');const pathsToAnimate=Array.from(paths).filter(el=>{const fill=el.style.fill||el.getAttribute('fill');if(fill==='rgb(195,153,103)')return false;if(el.closest('.my-head-wrapper'))return false;return true;});pathsToAnimate.forEach(el=>{el.style.strokeWidth='2px';el.style.fillRule='nonzero';});function hslToRgb(h,s,l){let c=(1-Math.abs(2*l-1))*s;let x=c*(1-Math.abs((h/60)%2-1));let m=l-c/2;let r=0,g=0,b=0;if(h<60)[r,g,b]=[c,x,0];else if(h<120)[r,g,b]=[x,c,0];else if(h<180)[r,g,b]=[0,c,x];else if(h<240)[r,g,b]=[0,x,c];else if(h<300)[r,g,b]=[x,0,c];else[r,g,b]=[c,0,x];r=Math.round((r+m)*255);g=Math.round((g+m)*255);b=Math.round((b+m)*255);return`rgb(${r},${g},${b})`;}let hue=0;function animateRainbow(){hue=(hue+1)%360;pathsToAnimate.forEach((el,i)=>{const pathHue=(hue+i*20)%360;el.style.stroke=hslToRgb(pathHue,1,0.5);});requestAnimationFrame(animateRainbow);}animateRainbow();const taglineDiv=document.querySelector('div.site-tagline[data-v-cac35620]');if(taglineDiv){taglineDiv.innerHTML=`games and stuff by <span class="rainbow-text">Neal</span>.<br>Mods by <span class="rainbow-text">Pin0ki0</span>`;}const rgbSpans=document.querySelectorAll('.rainbow-text');let hueText=0;function animateTextRainbow(){hueText=(hueText+1)%360;rgbSpans.forEach((el,i)=>{const spanHue=(hueText+i*50)%360;el.style.color=hslToRgb(spanHue,1,0.5);});requestAnimationFrame(animateTextRainbow);}animateTextRainbow();setTimeout(()=>{y.classList.remove('loading');document.querySelectorAll('#plantSwitch,#speedSlider,#headSwitch,#coffeeSwitch').forEach(el=>el.disabled=false);document.querySelectorAll('.theme-btn,.tab-btn').forEach(el=>el.disabled=false);document.querySelectorAll('.tab-btn').forEach(btn=>{btn.addEventListener('click',()=>{document.querySelectorAll('.tab-btn').forEach(b=>b.classList.remove('active'));document.querySelectorAll('.tab-content').forEach(c=>c.classList.remove('active'));btn.classList.add('active');document.getElementById(btn.dataset.tab+'-tab').classList.add('active')})});y.querySelector('.menu-header').addEventListener('mousedown',(g)=>{d=true;e=g.clientX-y.offsetLeft;f=g.clientY-y.offsetTop;g.preventDefault()});document.addEventListener('mousemove',(g)=>{if(d){y.style.left=(g.clientX-e)+'px';y.style.top=(g.clientY-f)+'px'}});document.addEventListener('mouseup',()=>d=false);y.querySelector('.minimize-btn').addEventListener('click',()=>{y.classList.toggle('minimized')});document.querySelectorAll('.theme-btn').forEach((h,i)=>{h.addEventListener('click',()=>{const j=['linear-gradient(135deg,#8B0000,#000000)','linear-gradient(135deg,#6A0080,#1A0040)'];y.style.background=j[i];currentTheme=i;updateMinimizeButtonTheme();updateThemeColors()})});const plantSwitch=document.getElementById('plantSwitch');const speedSlider=document.getElementById('speedSlider');const speedLabel=document.getElementById('speedLabel');plantSwitch.addEventListener('change',()=>{if(plantSwitch.checked){startPlantClicking();}else{clearInterval(intervals.plant);intervals.plant=null;}});speedSlider.addEventListener('input',()=>{speedLabel.textContent=speedSlider.value;if(plantSwitch.checked)startPlantClicking();});const headSwitch=document.getElementById('headSwitch');headSwitch.addEventListener('change',()=>{if(headSwitch.checked){intervals.head=setInterval(moveMouseSquare,1);}else{clearInterval(intervals.head);intervals.head=null;step=0;pos=0;}});const coffeeSwitch=document.getElementById('coffeeSwitch');coffeeSwitch.addEventListener('change',()=>{if(coffeeSwitch.checked){buyCoffeeMode();}else{restoreOriginal();}});updateMinimizeButtonTheme();updateThemeColors()},1000);
